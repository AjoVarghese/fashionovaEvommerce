    <script type="text/javascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js"></script>

        <script>
          jQuery('#form').validate({
            rules: {
                productname: "required",
                description:"required",
                quantity:"required",
                category:"required",
                price:"required",
                offerPrice:"required",
                productimage1:"required",
                productimage2:"required",
                productimage3:"required"
            },
             messages: {
                name: "Please enter your Product name",
                description:"Pleaser enter a description",
                quantity:"Please enter the quantity",
                category:"Please select one category",
                productimage1:"Please upload an image",
                productimage2:"Please upload an image",
                productimage3:"Please upload an image",
            price: {
            required: "Please enter the price",
            minlength: "Price must be 3 digit",
            maxlength:"Price must be 5 digit"
        },
        offerPrice: {
            required: "Please enter the Offer Price",
            minlength: "offerPrice must be 2 digit",
            maxlength:"offerPrice must be 5 digit"
        }
	},
            submitHandler: function (form) {
                form.submit();
            }
});
        </script>
